{
  "tasks": [
    {
      "type": "shell",
      "label": "g++-9 build active file",
      "group":{
        "kind": "build",
        "isDefault": true
      },
      "command": "/usr/bin/g++-9",
      "args":[
        "-g",
        "-O0",
        "-std=c++14",
        "${file}"
      ],
      "problemMatcher":[
        "$gcc"
      ],
    }
  ],
  "version": "2.0.0"
}
